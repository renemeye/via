<p id="notice"><%= notice %></p>

<h1>Plus City Zones</h1>

<table>
  <thead>
    <tr>
      <th>Dtzid</th>
      <th>City name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plus_city_zones.each do |plus_city_zone| %>
      <tr>
        <td><%= plus_city_zone.dtzid %></td>
        <td><%= plus_city_zone.city_name %></td>
        <td><%= link_to 'Show', plus_city_zone %></td>
        <td><%= link_to 'Edit', edit_plus_city_zone_path(plus_city_zone) %></td>
        <td><%= link_to 'Destroy', plus_city_zone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plus City Zone', new_plus_city_zone_path %>
